#include<stdio.h>
int main(){
    int numerator,denominator;
    printf("Enter a number (Numerator) :");
    scanf("%d",&numerator);
    printf("Enter another number (Denominator) :");
    scanf("%d",&denominator);
    if (numerator%denominator==0)
    printf("%d is divisible by %d",numerator,denominator);
    else 
    printf("%d is not divisible by %d",numerator,denominator);
    return 0;
}
